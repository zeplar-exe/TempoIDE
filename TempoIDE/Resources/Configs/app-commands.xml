<?xml version="1.0" encoding="utf-8"?>
<root>
    <menu Name="File">
        <command Name="New solution"/>
        <command Name="Open File..."/>
        <command Name="Close solution"/>
        <divider/>
        <command Name="Settings"/>
    </menu>
    
    <menu Name="Edit">
        <command Name="Copy">
            <Keybind>
                <Control>Control</Control>
                <Key>C</Key>
            </Keybind>
        </command>
        
        <command Name="Paste">
            <Keybind>
                <Control>Control</Control>
                <Key>V</Key>
            </Keybind>
        </command>
    
        <command Name="Cut">
            <Keybind>
                <Control>Control</Control>
                <Key>X</Key>
            </Keybind>
        </command>
        
        <command Name="Select All" NoMenu="True">
            <Keybind>
                <Control>Control</Control>
                <Key>A</Key>
            </Keybind>
        </command>
    </menu>
    
