<?xml version="1.0" encoding="utf-8"?>
<!-- NOTE! Whitespace in names is replaced with an empty string -->
<root>
    <command Name="Copy">
        <Keybind>
            <Key>LeftCtrl</Key>
            <Key>C</Key>
        </Keybind>
    </command>
    
    <command Name="Paste">
        <Keybind>
            <Key>LeftCtrl</Key>
            <Key>V</Key>
        </Keybind>
    </command>

    <command Name="Cut">
        <Keybind>
            <Key>LeftCtrl</Key>
            <Key>X</Key>
        </Keybind>
    </command>
    
    <command Name="Select All">
        <Keybind>
            <Key>LeftCtrl</Key>
            <Key>A</Key>
        </Keybind>
    </command>
</root>