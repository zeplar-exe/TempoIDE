<Application x:Class="TempoIDE.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Windows/MainWindow.xaml"
             Startup="App_OnStartup">
    <Application.Resources>
        <SolidColorBrush x:Key="DarkBorderColor" Color="#333333"/>
        <SolidColorBrush x:Key="DarkToolbarBackgroundColor" Color="#4A4A4A"/>
        <SolidColorBrush x:Key="DarkTextBoxBackgroundColor" Color="#1f1f1f"/>
    </Application.Resources>
</Application>